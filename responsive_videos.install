<?php

/**
 * @file
 * Install, update, and uninstall functions for the responsive_videos module.
 */

/**
 * Implements hook_field_schema().
 *
 */
function responsive_videos_field_schema($field) {
  $columns = array(
    'embed' => array('type' => 'text', 'size' => 'big', 'not null' => FALSE),
  );
  return array(
    'columns' => $columns,
  );
}
